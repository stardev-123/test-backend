{
  "name": "Onramp",
  "version": "1.0.0",
  "description": "Backand for Onramp crypto trading application.",
  "main": "index.js",
  "scripts": {
    "pretest": "cd database && NODE_ENV=test sequelize db:drop && NODE_ENV=test sequelize db:create && NODE_ENV=test sequelize db:migrate",
    "test": "NODE_ENV=test ./node_modules/istanbul/lib/cli.js cover ./node_modules/.bin/_mocha --exit ./tests/setupTests.js --report html -- -R spec ",
    "start": "node -r esm index.js",
    "deploy": "npm install && cd database && sequelize db:migrate && cd .. && node index",
    "deploy-qa": "export NODE_ENV=qa && npm install && cd database && sequelize db:migrate && cd .. && node index",
    "deploy-dev": "export NODE_ENV=dev && npm install && cd database && sequelize db:migrate && cd .. && node index"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
